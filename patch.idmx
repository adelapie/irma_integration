+#define __BINARY_OPN(N, OP, RES, OP1, OP2) \
+do {  \
+    __push (BLOCKCAST(N)(__typechk(unsigned char *, OP1))); \
+    __push (BLOCKCAST(N)(__typechk(unsigned char *, OP2))); \
+    __code (OP, N);  \
+    __code (POPN, N);  \
+    __code (STORE, __typechk(unsigned char *, RES), N); \
+} while (0)
